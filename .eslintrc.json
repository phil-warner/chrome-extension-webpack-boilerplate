{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jquery": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "StringUtils": true,
    "SelectionUtils": true,
    "chrome": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "no-console": ["error", { "allow": ["warn", "error", "info"] }]
  },
  "plugins": [
  ]
}
